#!/bin/sh

menu(){
    printf "4. four\n"
    printf "5. five\n"
    printf "6. six\n"
    printf "7. seven\n"
    printf "8. eight\n"
    printf "9. nine\n"
}

choice=$(menu | dmenu -f -i -p "üêàview" | cut -d. -f1)

case $choice in
    4)
        dwmc viewex 3
        ;;
    5)
        dwmc viewex 4
        ;;
    6)
        dwmc viewex 5
        ;;
    7)
        dwmc viewex 6
        ;;
    8)
        dwmc viewex 7
        ;;
    9)
        dwmc viewex 8
        ;;

    esac




